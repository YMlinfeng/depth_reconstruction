{
    // vqganlc_16384_multigpu
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "python": "/usr/bin/python3",
            "program": "/usr/local/lib/python3.9/dist-packages/torch/distributed/launch.py", 
            "args": [
                "--nproc-per-node=2",
                "--master-port=12345",  // üëà ÈÅøÂÖçÈªòËÆ§ÁöÑ 29500
                "--use-env",
                "train.py",  // üß† ‰Ω†ÁöÑ‰∏ªËÆ≠ÁªÉËÑöÊú¨
                "--validate_path=./output/vqganlc_16384",
                "--batch_size=4",
                "--epochs=10",
                "--lr=2e-4",
                "--num_workers=8",
                "--mid_channels=320",
                "--inp_channels=80",
                "--out_channels=80",
                "--checkpoint_dir=./checkpoints_vqganlc_16384",
                "--model=VQModel",
                "--save_interval=10",
                "--val_interval=4000",
                "--n_vision_words=16384",
                "--general_mode=vqgan",
                "--encoder_type=vqgan_lc",
                "--z_channels=4",
                "--vq_config_path=/mnt/bn/occupancy3d/workspace/mzj/mp_pretrain/vqganlc/vqgan_configs/vqganlc_16384.yaml"
            ],
            "env": {"CUDA_VISIBLE_DEVICES": "0,1"},
        }
    ]
}


